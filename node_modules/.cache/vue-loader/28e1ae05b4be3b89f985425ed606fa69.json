   <v-row>\n      <v-col col=\"12\" >\n        <v-text-field single-line solo placeholder=\"Ecrire ici pour ajouter une tâche.\" v-model=\"tache_ajout\"></v-text-field>\n        <v-btn dark color=\"indigo\" v-on:click=\"add\">Cliquer ici pour l'ajouter</v-btn>\n      </v-col>\n    </v-row>\n    <v-row v-if=\"liste_tache.length!=0\">\n      <v-col col=\"12\">\n        <table border=\"1\" width=\"100%\">\n          <tr>\n            <th class=\"indigo--text\">DESCRIPTION</th>\n            <th class=\"indigo--text\">VALIDATION</th>\n            <th class=\"indigo--text\">MODIFICATION</th>\n            <th class=\"indigo--text\">SUPPRESSION</th>\n          </tr>\n          <tr  v-for=\"(item,key) in liste_tache\"  v-bind:key=\"item\">\n            <td><b>{{ item }}</b></td>\n            <td style=\"text-align: center;\"><b class=\"green--text\">{{ validation[key] }}</b><v-spacer></v-spacer><b><input type=\"checkbox\" v-on:click=\"validate(key)\"><label>Valider</label></b></td>\n            <td style=\"text-align: center;\"><b><v-btn color=\"white white--text\" v-on:click=\"update(key)\">\n              <v-img class=\"shrink mr-2\" contain src=\"https://p.kindpng.com/picc/s/154-1541056_edit-edit-icon-svg-hd-png-download.png\"\n                transition=\"scale-transition\"\n                width=\"35\"/></v-btn></b></td>\n            <td style=\"text-align: center;\"><b><v-btn color=\"white white--text\" v-on:click=\"remove(key)\">\n              <v-img class=\"shrink mr-2\" contain src=\"https://media.istockphoto.com/vectors/trash-can-icon-logo-template-illustration-design-vector-eps-10-vector-id1223505692?k=6&m=1223505692&s=612x612&w=0&h=2VAEjHYfLRfeJG3wtPFRfW-bl990Y9CNtm6yhpyU9OU=\"\n                transition=\"scale-transition\"\n                width=\"35\"/></v-btn></b></td>\n          </tr>\n        </table>\n      </v-col>\n    </v-row>\n    <v-row v-else>\n      <v-col col=\"12\" >\n        <h1 class=\"red--text\">La liste des tâches est vide.</h1>\n        <center>\n        <v-img class=\"shrink mr-2\" contain src=\"https://i.pinimg.com/originals/24/a3/e0/24a3e0604e1c641d723d8f96564a1367.jpg\" \n            transition=\"scale-transition\"\n            width=\"650\"/>\n        </center>\n      </v-col>\n    </v-row>\n  </v-container>\n</div>\n</template>\n\n<script>\nexport default {\n  name: \"Home\",\n  data(){ \n    return{\n      liste_tache:[],\n      tache_ajout:\"\",\n      validation:[]\n    }\n  },\n  methods:{\n    add: function () {\n      if(this.liste_tache.indexOf(this.tache_ajout)!=-1){\n        window.alert(\"la tâche est déjà inscrite dans la liste.\");\n      }else{\n        this.liste_tache.push(this.tache_ajout);\n        this.validation.push(\"\");\n      }\n        this.tache_ajout=\"\";\n    },\n    remove: function (index) {\n      this.liste_tache.splice(index,1);\n      this.validation.splice(index,1);\n    },\n    update: function (index) {\n      var tache_up=prompt(\"Modifier la tâche\",\"\");\n\n      if(this.liste_tache.indexOf(tache_up)!=-1){\n        window.alert(\"la tâche est déjà inscrite dans la liste.\");\n      }else{\n        if(tache_up!=\"\"){\n          this.liste_tache.splice(index,1,tache_up);\n          this.validation.splice(index,1,\"\");\n        }\n      }\n    },\n    validate:function(index){\n        if(this.validation[index]==\"exécutée\"){\n          this.validation.splice(index,1,\"\");\n        }else{\n          this.validation.splice(index,1,\"exécutée\");\n        }\n    }\n  }\n}\n</script>\n"]}]}